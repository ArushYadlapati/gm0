{
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11.13"
    }
  },
  "buildCommand": "./build.sh",
  "outputDirectory": "build/html",
  "installCommand": "pip install --upgrade pip",
  "functions": {},
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
